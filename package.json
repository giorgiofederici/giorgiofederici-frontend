{
  "name": "giorgiofederici-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "ghp:build": "ng build --prod --output-path docs --base-href /giorgiofederici-frontend/ --source-map",
    "ghp:cp-index": "./node_modules/.bin/shx cp ./docs/index.html ./docs/404.html",
    "ghp": "npm run ghp:build && npm run ghp:cp-index",
    "test": "ng test",
    "test:ci": "ng test --watch=false --code-coverage && cat ./coverage/giorgiofederici-frontend/lcov.info | coveralls",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lite-server:local": "./node_modules/.bin/lite-server -c ./bs-config.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.0",
    "@angular/cdk": "^8.1.2",
    "@angular/common": "^8.2.0",
    "@angular/compiler": "^8.2.0",
    "@angular/core": "^8.2.0",
    "@angular/forms": "^8.2.0",
    "@angular/material": "^8.1.2",
    "@angular/platform-browser": "^8.2.0",
    "@angular/platform-browser-dynamic": "^8.2.0",
    "@angular/router": "^8.2.0",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-brands-svg-icons": "^5.10.1",
    "@fortawesome/free-regular-svg-icons": "^5.10.1",
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "@ngrx/effects": "^8.2.0",
    "@ngrx/entity": "^8.2.0",
    "@ngrx/router-store": "^8.2.0",
    "@ngrx/store": "^8.2.0",
    "@ngrx/store-devtools": "^8.2.0",
    "@ngx-i18nsupport/tooling": "^8.0.3",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "ngrx-store-freeze": "^0.2.4",
    "ngx-cookie-service": "^2.2.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular/cli": "^8.2.0",
    "@angular/compiler-cli": "^8.2.0",
    "@angular/language-service": "^8.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "coveralls": "^3.0.6",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "lite-server": "^2.5.3",
    "protractor": "~5.4.0",
    "shx": "^0.3.2",
    "source-map-explorer": "^2.0.1",
    "ts-node": "^8.3.0",
    "tslint": "~5.11.0",
    "typescript": "^3.5.3"
  }
}
